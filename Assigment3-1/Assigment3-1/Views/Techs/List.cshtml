@model List<Technician>
@{
    ViewBag.Title = "Technician Manager";
}
<h1>Technician Manager</h1>

<table class="table" table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>phone</th>
            <th>Release Date</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var tech in Model)
        {
            <tr>
                <td>@tech.Name</td>
                <td>@tech.Email</td>
                <td>@tech.Phone</td>
                <td>@tech.ReleaseDate.ToShortDateString()</td>
                <td>
                    <a asp-action="edit"
                   asp-route-id="@tech.TechnicianID"
                   class="btn btn-primary">Edit</a>
                </td>
                <td>
                    <a asp-action="Delete" asp-route-id="@tech.TechnicianID"
                   class="btn btn-primary">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Add" class="btn btn-primary mb-3">Adds Tech</a>